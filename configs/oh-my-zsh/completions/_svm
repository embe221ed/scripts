    local commands; commands=()
    _describe -t commands 'svm remote-list commands' commands "$@"
}
(( $+functions[_svm__show_commands] )) ||
_svm__show_commands() {
    local commands; commands=()
    _describe -t commands 'svm show commands' commands "$@"
}
(( $+functions[_svm__uninstall_commands] )) ||
_svm__uninstall_commands() {
    local commands; commands=()
    _describe -t commands 'svm uninstall commands' commands "$@"
}
(( $+functions[_svm__unset_commands] )) ||
_svm__unset_commands() {
    local commands; commands=()
    _describe -t commands 'svm unset commands' commands "$@"
}
(( $+functions[_svm__use_commands] )) ||
_svm__use_commands() {
    local commands; commands=()
    _describe -t commands 'svm use commands' commands "$@"
}

if [ "$funcstack[1]" = "_svm" ]; then
    _svm "$@"
else
    compdef _svm svm
fi

