    local commands; commands=()
    _describe -t commands 'svm help remote-list commands' commands "$@"
}
(( $+functions[_svm__help__show_commands] )) ||
_svm__help__show_commands() {
    local commands; commands=()
    _describe -t commands 'svm help show commands' commands "$@"
}
(( $+functions[_svm__help__uninstall_commands] )) ||
_svm__help__uninstall_commands() {
    local commands; commands=()
    _describe -t commands 'svm help uninstall commands' commands "$@"
}
(( $+functions[_svm__help__unset_commands] )) ||
_svm__help__unset_commands() {
    local commands; commands=()
    _describe -t commands 'svm help unset commands' commands "$@"
}
(( $+functions[_svm__help__use_commands] )) ||
_svm__help__use_commands() {
    local commands; commands=()
    _describe -t commands 'svm help use commands' commands "$@"
}
(( $+functions[_svm__install_commands] )) ||
_svm__install_commands() {
    local commands; commands=()
    _describe -t commands 'svm install commands' commands "$@"
}
